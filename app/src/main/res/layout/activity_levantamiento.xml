<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-177dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/lblExpdiente"
                android:layout_width="100dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="60dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/expediente"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblOperador"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/operador"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblExpdiente"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/txtLev"
                android:layout_width="251dp"
                android:layout_height="33dp"
                android:layout_marginStart="90dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="90dp"
                android:layout_marginBottom="125dp"
                android:gravity="center"
                android:text="@string/levantamiento_info"
                android:textColor="#1B4965"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/lblProyecto"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/proyecto"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblOperador"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblTipoinfra"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/tipoinfra"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblProyecto"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblIdinfrapt"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/id_infra_pt"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblTipoinfra"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblCaracter"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/caracter"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblIdinfrapt"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblEstado"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/estado"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblCaracter"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblAdecuacion"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/adecuacion"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblEstado"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblTipoadec"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/tipoadec"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblAdecuacion"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblCalidad"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/calidad"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblTipoadec"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblObserv"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/observ"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblCalidad"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblEste"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/Este"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblObserv"
                tools:ignore="TextViewEdits" />

            <TextView
                android:id="@+id/lblNorte"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:background="#BEE9E8"
                android:cursorVisible="false"
                android:ellipsize="none"
                android:freezesText="false"
                android:gravity="center"
                android:linksClickable="false"
                android:text="@string/Norte"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#000000"
                app:autoSizeTextType="none"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblEste"
                tools:ignore="TextViewEdits" />

            <EditText
                android:id="@+id/txtExpediente"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/expededit"
                android:inputType="text"
                android:maxLength="20"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/lblExpdiente"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/txtTipoinfra"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/tipoedit"
                android:inputType="text"
                android:maxLength="50"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/lblTipoinfra"
                app:layout_constraintTop_toBottomOf="@+id/txtProyecto" />

            <EditText
                android:id="@+id/txtProyecto"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/proyectoedit"
                android:inputType="text"
                android:maxLength="200"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/lblProyecto"
                app:layout_constraintTop_toBottomOf="@+id/txtOperador" />

            <EditText
                android:id="@+id/txtOperador"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/operadoredit"
                android:inputType="text"
                android:maxLength="100"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/lblOperador"
                app:layout_constraintTop_toBottomOf="@+id/txtExpediente" />

            <EditText
                android:id="@+id/txtIdinfra"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/idinfra"
                android:inputType="text"
                android:maxLength="20"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/lblIdinfrapt"
                app:layout_constraintTop_toBottomOf="@+id/txtTipoinfra" />

            <EditText
                android:id="@+id/txtCaracter"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="44dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/caracteristica"
                android:inputType="text"
                android:maxLength="255"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/lblCaracter"
                app:layout_constraintTop_toBottomOf="@+id/txtIdinfra" />

            <EditText
                android:id="@+id/txtEste"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="44dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/esteTXT"
                android:inputType="numberDecimal"
                android:maxLength="8"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/lblCaracter"
                app:layout_constraintTop_toBottomOf="@+id/txtObervaciones" />

            <EditText
                android:id="@+id/txtNorte"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="44dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/norteTXT"
                android:inputType="numberDecimal"
                android:maxLength="8"
                android:minHeight="48dp"
                android:textColorHint="#757575"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/lblCaracter"
                app:layout_constraintTop_toBottomOf="@+id/txtEste" />

            <Button
                android:id="@+id/btnRegistrar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="158dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="159dp"
                android:backgroundTint="#62B6CB"
                android:text="@string/registrarCoordenadas"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNorte" />

            <Button
                android:id="@+id/btnTomar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="158dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="159dp"
                android:backgroundTint="#62B6CB"
                android:text="@string/tomar"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnRegistrar" />

            <Spinner
                android:id="@+id/spiEstado"
                android:layout_width="211dp"
                android:layout_height="48dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="44dp"
                android:entries="@array/dom_estado"
                android:splitMotionEvents="false"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/lblEstado"
                app:layout_constraintTop_toBottomOf="@+id/txtCaracter" />

            <Spinner
                android:id="@+id/spiAdecuacion"
                android:layout_width="211dp"
                android:layout_height="48dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="44dp"
                android:entries="@array/dom_adecu"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/lblAdecuacion"
                app:layout_constraintTop_toBottomOf="@+id/spiEstado" />

            <EditText
                android:id="@+id/txtTipoadec"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginStart="38dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="48dp"
                android:autofillHints=""
                android:ems="10"
                android:hint="@string/tipoadecu"
